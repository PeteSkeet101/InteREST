        <name>Test Connection</name>
        <onclick>testConn()</onclick>
        <order>500</order>
        <script><![CDATA[action.setRedirectURL(current);
current.update();

try {
    testConnection = new InteRESTIncidentUtils(current.sys_id.toString());
    var testResult = testConnection.testConn();

    switch (testResult) {

        case 'offline':
            gs.addErrorMessage("Error: Host not responding / offline ");
            break;
        case 'online':
              break;
        case 'badHost':
            gs.addErrorMessage('Error: Host not found, please check the host name');
            break;

        case 'badUserPassword':
            gs.addErrorMessage('Error: username / password not accepted, please check and retry.');
            break;
        default:
            gs.addErrorMessage('Unknown errror: ' + testResult);

    }
} catch (err) {
    gs.info('UI action error is ' + err);
}]]></script>
